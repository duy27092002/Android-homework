<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/second_nav_graph"
    app:startDestination="@id/helloFragment">

    <fragment
        android:id="@+id/helloFragment"
        android:name="t3h.android.navgraph.HelloFragment"
        android:label="fragment_hello"
        tools:layout="@layout/fragment_hello" >
        <action
            android:id="@+id/action_helloFragment_to_hiFragment"
            app:destination="@id/hiFragment" />
    </fragment>
    <fragment
        android:id="@+id/hiFragment"
        android:name="t3h.android.navgraph.HiFragment"
        android:label="fragment_hi"
        tools:layout="@layout/fragment_hi" />
</navigation>